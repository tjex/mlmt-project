    "# Roughly 20% of our data.\n",
    "train_data_act = data_act[data_act['sequence_name'].isin(['B01', 'B02', 'B03', 'B04',\n",
    "       'B05', 'C01', 'C02', 'C03', 'C04', 'C05', 'D01', 'D02', 'D03',\n",
